{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "ambassador-gen3": "quay.io/datawire/ambassador:1.4.2",
    "arborist": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/arborist:2022.03", 
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "fence": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/fence:2022.03", 
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "guppy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/guppy:2022.03", 
    "hatchery": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/hatchery:2022.03", 
    "indexd": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/indexd:2022.03", 
    "manifestservice": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/manifestservice:2022.03", 
    "metadata": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/metadata-service:2022.03", 
    "peregrine": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/peregrine:2022.03", 
    "pidgin": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/pidgin:2022.03",
    "portal": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/data-portal:3.22.1",
    "revproxy": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/nginx:2022.03",
    "sheepdog": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sheepdog:2022.03",
    "ssjdispatcher": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/ssjdispatcher:2022.03", 
    "sower": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/sower:2022.03",
    "spark": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/gen3-spark:2022.03",
    "tube": "quay.io/cdis/tube:master",
    "wts": "707767160287.dkr.ecr.us-east-1.amazonaws.com/gen3/workspace-token-service:2022.03" 
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "sower": [
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "case"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-import",
      "action": "import",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-import:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sheepdog-creds-volume",
            "readOnly": true,
            "mountPath": "/sheepdog-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "sheepdog-creds-volume",
          "secret": {
            "secretName": "sheepdog-creds"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "hatchery": {
    "user-namespace": "jupyter-pods-spencer",
    "sub-dir": "/lw-workspace",
    "user-volume-size": "10Gi",
    "sidecar": {
      "cpu-limit": "1.0",
      "memory-limit": "256Mi",
      "image": "quay.io/cdis/gen3fuse-sidecar:master",
      "env": {
        "NAMESPACE": "spencer",
        "HOSTNAME": "spencer.planx-pla.net"
      },
      "args": [],
      "command": [
        "/bin/bash",
        "/sidecarDockerrun.sh"
      ],
      "lifecycle-pre-stop": [
        "su",
        "-c",
        "echo test",
        "-s",
        "/bin/sh",
        "root"
      ]
    },
    "containers": [
      {
        "target-port": 8787,
        "cpu-limit": "1.0",
        "memory-limit": "512Mi",
        "name": "R Studio",
        "image": "heliumdatastage/rstudio-server:1",
        "env": {
          "DISABLE_AUTH": "true"
        },
        "args": [],
        "path-rewrite": "/",
        "use-tls": "false",
        "ready-probe": "/"
      },
      {
        "target-port": 8888,
        "cpu-limit": "1.0",
        "memory-limit": "512Mi",
        "name": "Jupyter Notebook Bio Python",
        "image": "quay.io/occ_data/jupyternotebook:1.7.2",
        "env": {},
        "args": [
          "--NotebookApp.base_url=/lw-workspace/proxy/",
          "--NotebookApp.password=''",
          "--NotebookApp.token=''"
        ],
        "command": [
          "start-notebook.sh"
        ],
        "path-rewrite": "/lw-workspace/proxy/",
        "use-tls": "false",
        "ready-probe": "/lw-workspace/proxy/",
        "lifecycle-post-start": [
          "/bin/sh",
          "-c",
          "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
        ],
        "user-uid": 1000,
        "fs-gid": 100,
        "user-volume-location": "/home/jovyan/pd"
      }
    ]
  },
  "global": {
    "environment": "devplanetv1",
    "hostname": "spencer.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "dispatcher_job_num": "10",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/dev/user.yaml",
    "tier_access_level": "regular",
    "tier_access_limit": 50,
    "public_datasets": true,
    "netpolicy": "on"
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "spencer_etl",
        "type": "case"
      },
      {
        "index": "spencer_file",
        "type": "file"
      }
    ],
    "config_index": "spencer_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "fence": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "indexd": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "revproxy": {
      "strategy": "auto",
      "min": 1,
      "max": 1
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 40
    },
    "metadata": {
      "strategy": "auto",
      "min": 1,
      "max": 1,
      "targetCpu": 40
    }
  }
}
