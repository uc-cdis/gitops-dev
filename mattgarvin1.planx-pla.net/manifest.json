{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:master",
    "arranger": "quay.io/cdis/arranger:master",
    "arranger-adminapi": "quay.io/cdis/arranger-server:master",
    "arranger-dashboard": "quay.io/cdis/arranger-dashboard:master",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "fence": "quay.io/cdis/fence:master",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "indexd": "quay.io/cdis/indexd:master",
    "jupyterhub": "quay.io/occ_data/jupyterhub:master",
    "mariner": "quay.io/cdis/mariner-server:feat_k8s",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:master",
    "peregrine": "quay.io/cdis/peregrine:master",
    "wts": "quay.io/cdis/workspace-token-service:master",
    "pidgin": "quay.io/cdis/pidgin:master",
    "portal": "quay.io/cdis/data-portal:master",
    "revproxy": "quay.io/cdis/nginx:1.15.5-ctds",
    "sheepdog": "quay.io/cdis/sheepdog:master",
    "sower": "quay.io/cdis/sower:master",
    "spark": "quay.io/cdis/gen3-spark:master",
    "tube": "quay.io/cdis/tube:master",
    "dashboard": "quay.io/cdis/gen3-statics:master"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "sower": [
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "case"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-import",
      "action": "import",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-import:master",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "global": {
    "environment": "devplanetv1",
    "hostname": "mattgarvin1.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "portal_app": "dev",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "netpolicy": "on",
    "useryaml_s3path": "s3://cdis-gen3-users/dev/user.yaml",
    "dispatcher_job_num": "10"
  },
  "arranger": {
    "project_id": "dev",
    "auth_filter_field": "gen3_resource_path",
    "auth_filter_node_types": [
      "subject"
    ]
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "canary": {
    "default": 0
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "mariner": {
    "secrets": {
      "awsusercreds": {
        "name": "workflow-bot-g3auto",
        "key": "awsusercreds.json"
      }
    },
    "jobs": {
      "engine": {
        "labels": {
          "app": "mariner-engine"
        },
        "serviceaccount": "mariner-service-account",
        "restart_policy": "never"
      },
      "task": {
        "labels": {
          "app": "mariner-task"
        },
        "restart_policy": "never"
      }
    },
    "containers": {
      "engine": {
        "name": "mariner-engine",
        "image": "quay.io/cdis/mariner-engine:feat_k8s",
        "pull_policy": "always",
        "command": ["/bin/sh"],
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "400m"
          }
        }
      },
      "s3sidecar": {
        "name": "mariner-s3sidecar",
        "image": "quay.io/cdis/mariner-s3sidecar:feat_k8s",
        "pull_policy": "always",
        "command": ["/bin/sh", "./s3sidecarDockerrun.sh"],
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "gen3fusesidecar": {
        "name": "mariner-gen3fusesidecar",
        "image": "quay.io/cdis/gen3fuse-sidecar:feat_mariner_bash",
        "pull_policy": "always",
        "command": ["/bin/bash", "/marinerRun.sh"],
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "task": {
        "name": "mariner-task",
        "image": "alpine",
        "pull_policy": "always",
        "command": ["/bin/sh"],
        "resources": {
          "limits": {
            "memory": "512Mi",
            "cpu": "900m"
          }
        }
      }
    }
  }
}
