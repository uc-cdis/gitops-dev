    "cpu-limit": "0.4",
    "memory-limit": "256Mi",
    "image": "quay.io/cdis/gen3fuse-sidecar:fix_squid",
    "env": {
      "NAMESPACE": "reuben",
      "HOSTNAME": "reuben.planx-pla.net"
    },
    "args": [],
    "command": [
      "/bin/bash",
      "/sidecarDockerrun.sh"
    ],
    "lifecycle-pre-stop": [
      "su",
      "-c",
      "echo test",
      "-s",
      "/bin/sh",
      "root"
    ]
  },
