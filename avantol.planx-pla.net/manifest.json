            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "activeDeadlineSeconds": 86400,
      "serviceAccountName": "jobs-avantol-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:feat_gen3-update",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
